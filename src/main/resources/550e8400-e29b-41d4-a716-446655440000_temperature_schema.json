{
	"title": "Generic sensor schema",
	"type": "object",
	"properties": {
		"stream": {
			"type": "string"
		},
		"sensor": {
			"type": "string"
		},
		"values": {
			"type": "array",
			"minItems" : 1,
			"items": { 
				"type": "object",
				"properties": {
					"time": {
						"type": "string"
					},
					"components" : {
						"type": "object",
						"properties": {
							"c0" : {
								"type" : "string"
							}
						},
						"required": ["c0"]
					}
				}
			}
		}
	},
	"required": ["stream", "sensor", "values"]
}